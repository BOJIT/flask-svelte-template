{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": ["./**/*"],
	"exclude": ["node_modules/*", "__sapper__/*", "static/*"],
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"module": "esnext",
		"baseUrl": "./",
		"paths": {
			"store/*": ["ts/store/*"],
			"scss/*": ["scss/*"],
			"global/*": ["ts/global/*"],
			"s/*": ["svelte/*"],
		}
	}
}
