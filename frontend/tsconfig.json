{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": ["./**/*", "../node_modules/**/*"],
	"exclude": ["node_modules/*", "__sapper__/*", "static/*"],
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"module": "esnext",
		"baseUrl": "./",
		"paths": {
			"store/*": ["ts/store/*"],
			"styles/*": ["scss/*"],
			"components/*": ["svelte/components/*"],
			"global/*": ["ts/global/*"]
		}
	}
}
