{
  "name": "flask-svelte-template",
  "version": "0.0.0",
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.11.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.3.4",
    "noflo-component-loader": "^0.4.1",
    "path-browserify": "^1.0.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "svelte": "^3.31.2",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.3.0",
    "ts-loader": "^8.0.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "build": "npx webpack --config frontend/webpack.prod.js",
    "dev": "npx concurrently 'npm:flask-dev' 'npm:svelte-dev'",
    "flask-dev": "cd ./backend && cross-env FLASK_APP=backend/server.py FLASK_ENV=development pipenv run python -m flask run --host 0.0.0.0",
    "svelte-dev": "npx webpack --config frontend/webpack.dev.js"
  },
  "dependencies": {
    "@fontsource/comfortaa": "^4.5.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "bulma": "^0.9.3",
    "localforage": "^1.10.0",
    "simplebar": "^5.3.6",
    "svelte-awesome": "^2.4.2"
  }
}
